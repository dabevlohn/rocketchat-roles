<h1 class="text-3xl font-bold underline">Users</h1>
<!-- templates/users.html -->
<div id="users">
<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Roles</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody id="users-content">
        {% for user in users %}
            {% include "user" %}
        {% endfor %}
    </tbody>
</table>
</div>
